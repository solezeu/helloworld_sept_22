            steps {
                echo 'Hello'
            }
        }
        stage('Build') {
            steps {
                echo 'Hello build'
            }
        }
        stage('Test') {
            steps {
                echo 'Hello test'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Hello deploy'
            }
        }
    }
}
